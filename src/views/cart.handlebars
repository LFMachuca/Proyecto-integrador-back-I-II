<h1 class="bg-success-subtle p-2 text-center text-success w-100">CART</h1>
<section class="d-flex flex-wrap justify-content-evenly">
  {{#each cart}}
    <div class="card m-1" style="width: 18rem;">
      <div class="card-body">
        <h5 class="card-title">{{this.product_id.title}}</h5>
        <p class="card-text">USD:{{this.product_id.price}}</p>
        <p class="card-text">Quantity:{{this.quantity}}</p>
      </div>
    </div>
  {{/each}}
</section>
